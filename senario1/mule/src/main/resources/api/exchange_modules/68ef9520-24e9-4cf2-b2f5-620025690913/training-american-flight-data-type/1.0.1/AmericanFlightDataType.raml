#%RAML 1.0 DataType

type: object
properties: 
  ID?: integer
  code: string
  price: number
  departureDate:
    type: string
    pattern: \d{4}\/\d{2}\/\d{2}
    description: Date format has to be YYYY/MM/DD format
  origin: string
  destination: string
  emptySeats: integer
  plane:
    type: object
    required: false
    properties:
      type: string
      totalSeats: integer
